<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="826.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ReportController">
   <children>
      <Label layoutX="17.0" layoutY="16.0" prefHeight="34.0" prefWidth="117.0" text="Reports">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="592.0" layoutY="454.0" mnemonicParsing="false" onAction="#backButtonOnClick" prefHeight="26.0" prefWidth="97.0" text="Back" />
      <ComboBox fx:id="contactName" layoutX="640.0" layoutY="73.0" onAction="#contactScheduleDropDown" prefWidth="150.0" />
      <Label layoutX="513.0" layoutY="77.0" prefHeight="17.0" prefWidth="103.0" text="Select A Contact" />
      <TableView fx:id="contactScheduleTable" layoutX="26.0" layoutY="113.0" prefHeight="105.0" prefWidth="775.0">
         <columns>
            <TableColumn fx:id="appointmentId" prefWidth="43.0" text="ID" />
            <TableColumn fx:id="appointmentTitle" prefWidth="79.0" text="Title" />
            <TableColumn fx:id="appointmentType" prefWidth="73.0" text="Type " />
            <TableColumn fx:id="appointmentDescription" prefWidth="161.0" text="Description" />
            <TableColumn fx:id="appointmentLocation" prefWidth="86.0" text="Location" />
            <TableColumn fx:id="appointmentStart" prefWidth="124.0" text="Start Date/Time" />
            <TableColumn fx:id="appointmentEnd" prefWidth="121.0" text="End Date/Time" />
            <TableColumn fx:id="appointmentCustomerId" prefWidth="87.0" text="Customer ID" />
         </columns>
      </TableView>
      <TableView fx:id="appointmentByMonthTable" layoutX="26.0" layoutY="243.0" prefHeight="200.0" prefWidth="440.0">
         <columns>
            <TableColumn fx:id="appointmentMonth" prefWidth="125.0" text="Appointment Month" />
            <TableColumn fx:id="appointmentTypeColumn" prefWidth="157.0" text="Appointment Type" />
            <TableColumn fx:id="typeTotal" prefWidth="157.0" text="Total Appointments" />
         </columns>
      </TableView>
      <TableView fx:id="customerByDivisionTable" layoutX="533.0" layoutY="243.0" prefHeight="200.0" prefWidth="267.0">
         <columns>
            <TableColumn fx:id="divisionName" prefWidth="145.0" text="Division Name" />
            <TableColumn fx:id="divisionTotal" prefWidth="121.0" text="Total Customers" />
         </columns>
      </TableView>
      <Button fx:id="logoutButton" layoutX="704.0" layoutY="454.0" mnemonicParsing="false" onAction="#logoutOnClick" prefHeight="26.0" prefWidth="97.0" text="Logout" />
   </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Login">
   <children>
      <Label fx:id="loginLabel" alignment="CENTER" layoutX="184.0" layoutY="38.0" prefHeight="63.0" prefWidth="193.0" text="Login">
         <font>
            <Font name="System Bold" size="32.0" />
         </font>
      </Label>
      <Label fx:id="usernameLabel" layoutX="69.0" layoutY="145.0" prefHeight="36.0" prefWidth="150.0" text="Username">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <Label fx:id="passwordLabel" layoutX="69.0" layoutY="200.0" prefHeight="36.0" prefWidth="150.0" text="Password">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <TextField fx:id="usernameText" layoutX="281.0" layoutY="145.0" prefHeight="36.0" prefWidth="242.0" />
      <PasswordField fx:id="passwordText" layoutX="281.0" layoutY="196.0" prefHeight="36.0" prefWidth="242.0" />
      <Label fx:id="timeZoneLabel" layoutX="69.0" layoutY="295.0" prefHeight="36.0" prefWidth="186.0" text="Time Zone">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <Label fx:id="timeZoneDisplay" disable="true" layoutX="281.0" layoutY="295.0" prefHeight="36.0" prefWidth="242.0">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <Button fx:id="loginButton" layoutX="281.0" layoutY="258.0" mnemonicParsing="false" onAction="#loginOnClick" prefHeight="28.0" prefWidth="110.0" text="Login" />
      <Button fx:id="exitButton" layoutX="410.0" layoutY="258.0" mnemonicParsing="false" onAction="#exitOnClick" prefHeight="28.0" prefWidth="110.0" text="Exit" />
   </children>
</AnchorPane>
